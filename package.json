{
  "name": "eco-nexus",
  "version": "1.0.0",
  "description": "Carbon Marketplace MVP - Sustainable Choice Operating System",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "install:all": "cd client && npm install && cd ../server && npm install",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "deploy:setup": "node scripts/auto-deploy.js",
    "deploy:railway": "cd server && railway up",
    "deploy:vercel": "cd client && vercel --prod"
  },
  "keywords": [
    "sustainability",
    "multi-agent",
    "blockchain",
    "carbon-marketplace",
    "mongodb",
    "snowflake"
  ],
  "author": "",
  "license": "MIT"
}

